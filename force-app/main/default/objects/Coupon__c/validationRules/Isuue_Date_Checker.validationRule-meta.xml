<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Isuue_Date_Checker</fullName>
    <active>true</active>
    <description>Enter Valid Issue Date</description>
    <errorConditionFormula>Issue_Date__c  &gt;=  Expiry_Date__c</errorConditionFormula>
    <errorMessage>Please Enter Valid Issue Date. Issue date should not be ahead of expiry date.</errorMessage>
</ValidationRule>
